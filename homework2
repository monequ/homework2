class Hero:
    def __init__(self, name, power):
        self.name = name
        self.power = power


    def start(self):
        print(f"Персонаж {self.name} создан")

    def shutdown(self):
        print(f"{self.name} Готов к бою")

  
Hero1 = Hero("Саб-зибо","Сила: Льда")


print(Hero1.name)
print(Hero1.power)


Hero1.start()
Hero1.shutdown()